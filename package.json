{
  "name": "filemk",
  "version": "1.0.2",
  "description": "A simple tool to create files from the command line",
  "main": "index.js",
  "scripts": {
    "test-watch": "node ./node_modules/mocha/bin/mocha --watch __tests__ lib",
    "test": "node ./node_modules/mocha/bin/mocha __tests__",
    "cov": "nyc npm run test",
    "coverage": "nyc npm run test --reporter=text-lcov | coveralls" 
  },
  "keywords": [
    "mk",
    "filemk",
    "file",
    "create",
    "makefile",
    "filemake"
  ],
  "bin": {
    "mk": "./main.js",
    "filemk": "./main.js"
  },
  "author": "Wisdom Ogwu (iammadab)",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
